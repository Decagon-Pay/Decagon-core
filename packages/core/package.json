{
  "name": "@decagon/core",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "description": "Effectful business logic core for Decagon",
  "repository": {
    "type": "git",
    "url": "https://github.com/decagon-protocol/decagon-core.git",
    "directory": "packages/core"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./capabilities": {
      "types": "./dist/capabilities/index.d.ts",
      "import": "./dist/capabilities/index.js"
    },
    "./workflows": {
      "types": "./dist/workflows/index.d.ts",
      "import": "./dist/workflows/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'lint passed'",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@decagon/x402": "workspace:*",
    "effect": "^3.0.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0"
  }
}
